Create Table #Temp1 (
Replastname varchar(500),
QuoteTotal int null,
OrderTotal int null)

Insert into #temp1 (OrderTotal)
select
	
	--quotedate,
	--[customerpo#],
	--replastname,
	--quoter,
	--statusoforder,
	 count(quoteno) as count
	
from 
	ussl3k001.sqlprivy.dbo.mporders 
where 
	statusoforder   in ('Order') and
	 quotedate between @startdate and @enddate
--group by
	--replastname
	--order by replastname


	select
	
	--quotedate,
	--[customerpo#],
	--replastname,
	--quoter,
	--statusoforder,
	 count(quoteno) as count
	 into #temp2
