Replastname varchar(500),
QuoteTotal int null,
OrderTotal int null)

Insert into #temp1 (replastname,OrderTotal)
select
	
	--quotedate,
	--[customerpo#],
	replastname,
	--quoter,
	--statusoforder,
	 count(quoteno) as count
	
from 
	ussl3k001.sqlprivy.dbo.mporders 
where 
	statusoforder   in ('Order') and
	 quotedate between @startdate and @enddate
group by
	replastname
	--order by replastname


	select
	
	--quotedate,
	--[customerpo#],
	replastname,
	--quoter,
	--statusoforder,
	 count(quoteno) as count
	 into #temp2
from 
	ussl3k001.sqlprivy.dbo.mporders 
where 
	statusoforder not  in ('Cancelled') and
	 quotedate between @startdate and @enddate
group by
	replastname
	--order by replastname


	Update #temp1
	set Quotetotal = count
	from #temp2
	left join #temp1 on #temp2.replastname = #temp1.replastname


	Select * from #temp1</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="replastname"><DataField>Replastname</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="QuoteTotal"><DataField>QuoteTotal</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="OrderTotal"><DataField>OrderTotal</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="PercentClose"><Value>=(Fields!OrderTotal.Value/Fields!QuoteTotal.Value)</Value></Field></Fields></DataSet><DataSet Name="TotalOrderbyRep"><Query><DataSourceName>SQLPrivy</DataSourceName><CommandText>select
	
