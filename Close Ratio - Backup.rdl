    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="replastname">
          <DataField>replastname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="statusoforder">
          <DataField>statusoforder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SQLPrivy</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select
	
	--quotedate,
	--[customerpo#],
	replastname,
	--quoter,
	statusoforder,
	 count(quoteno) as count
from 
	mporders 
where 
	statusoforder not in ('quote') and quotedate between @startdate and @enddate
group by
	replastname,statusoforder</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!startdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!enddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
